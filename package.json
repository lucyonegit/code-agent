{
  "name": "react-core",
  "version": "1.0.0",
  "description": "A fully business-decoupled ReAct Agent core framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node --esm",
    "typecheck": "tsc --noEmit",
    "example:basic": "npx tsx src/examples/basic-usage.ts",
    "example:planner": "npx tsx src/examples/planner-usage.ts",
    "serve": "npx tsx src/server/index.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,json}\""
  },
  "keywords": [
    "react-agent",
    "langchain",
    "llm",
    "ai-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^1.1.0",
    "@langchain/core": "^1.1.5",
    "@langchain/langgraph": "^1.0.5",
    "@langchain/openai": "^1.2.0",
    "dotenv": "^17.2.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.19.0"
  }
}
